<Script ConfigType="Database" LoaderType="Beef.CodeGen.Loaders.TableConfigLoader, Beef.CodeGen.Core">
  <Generate GenType="Beef.CodeGen.Generators.DatabaseGetCodeGenerator" Template="DbSpTableGet_sql.hbs" FileName="{{StoredProcedureName}}.sql" OutDir="{{Root.PathDatabase}}/Schema/{{Parent.Schema}}/Stored Procedures" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseGetCollCodeGenerator" Template="DbSpTableGetColl_sql.hbs" FileName="{{StoredProcedureName}}.sql" OutDir="{{Root.PathDatabase}}/Schema/{{Parent.Schema}}/Stored Procedures" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseCreateCodeGenerator" Template="DbSpTableCreate_sql.hbs" FileName="{{StoredProcedureName}}.sql" OutDir="{{Root.PathDatabase}}/Schema/{{Parent.Schema}}/Stored Procedures" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseUpdateCodeGenerator" Template="DbSpTableUpdate_sql.hbs" FileName="{{StoredProcedureName}}.sql" OutDir="{{Root.PathDatabase}}/Schema/{{Parent.Schema}}/Stored Procedures" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseUpsertCodeGenerator" Template="DbSpTableUpsert_sql.hbs" FileName="{{StoredProcedureName}}.sql" OutDir="{{Root.PathDatabase}}/Schema/{{Parent.Schema}}/Stored Procedures" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseDeleteCodeGenerator" Template="DbSpTableDelete_sql.hbs" FileName="{{StoredProcedureName}}.sql" OutDir="{{Root.PathDatabase}}/Schema/{{Parent.Schema}}/Stored Procedures" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseMergeCodeGenerator" Template="DbSpTableMerge_sql.hbs" FileName="{{StoredProcedureName}}.sql" OutDir="{{Root.PathDatabase}}/Schema/{{Parent.Schema}}/Stored Procedures" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseUdtCodeGenerator" Template="DbUdtTableList_sql.hbs" FileName="udt{{Name}}List.sql" OutDir="{{Root.PathDatabase}}/Schema/{{Schema}}/Types/User-Defined Table Types" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseTvpCodeGenerator" Template="DbTvpTable_cs.hbs" FileName="{{Tvp}}DataTvp.cs" OutDir="{{Root.PathBusiness}}/Data" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseEfModelGenerator" Template="DbEfModel_cs.hbs" FileName="{{EFModelName}}.cs" OutDir="{{Root.PathBusiness}}/Data/EfModel" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseEfModelBuilderCodeGenerator" Template="DbEfModelBuilder_cs.hbs" FileName="ModelBuilderExtensions.cs" OutDir="{{Root.PathBusiness}}/Data/EfModel" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseQueryViewCodeGenerator" Template="DbVwQuery_sql.hbs" FileName="{{ViewName}}.sql" OutDir="{{Root.PathDatabase}}/Schema/{{ViewSchema}}/Views" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseCdcRootCodeGenerator" Template="DbCdcTrackingTableCreate_sql.hb" FileName="{{Root.CdcTrackingTableName}}.sql" OutDir="{{Root.PathDatabase}}/Schema/{{Root.CdcSchema}}/Tables" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseCdcRootCodeGenerator" Template="DbCdcTrackingUdt_sql.hb" FileName="udt{{Root.CdcTrackingTableName}}List.sql" OutDir="{{Root.PathDatabase}}/Schema/{{Root.CdcSchema}}/Types/User-Defined Table Types" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseCdcRootCodeGenerator" Template="DbCdcTrackingTvp_cs.hb" FileName="CdcTrackingDbMapper.cs" OutDir="{{Root.PathCdc}}/Data" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseCdcCodeGenerator" Template="DbCdcSpExecuteEnvelope_sql.hb" FileName="{{StoredProcedureName}}.sql" OutDir="{{Root.PathDatabase}}/Schema/{{CdcSchema}}/Stored Procedures" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseCdcCodeGenerator" Template="DbCdcEnvelopeTableCreate_sql.hb" FileName="{{EnvelopeTableName}}.sql" OutDir="{{Root.PathDatabase}}/Schema/{{CdcSchema}}/Tables" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseCdcCodeGenerator" Template="DbCdcEntity_cs.hb" FileName="{{ModelName}}Cdc.cs" OutDir="{{Root.PathCdc}}/Entities" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseCdcCodeGenerator" Template="DbCdcData_cs.hb" FileName="{{ModelName}}CdcData.cs" OutDir="{{Root.PathCdc}}/Data" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseCdcCodeGenerator" Template="DbCdcBackgroundService_cs.hb" FileName="{{ModelName}}CdcBackgroundService.cs" OutDir="{{Root.PathCdc}}/Services" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseCdcRootCodeGenerator" Template="DbCdcDataSce_cs.hb" FileName="ServiceCollectionsExtension.cs" OutDir="{{Root.PathCdc}}/Data" />
</Script>