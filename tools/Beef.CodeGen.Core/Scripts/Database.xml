<Script ConfigType="Database" LoaderType="Beef.CodeGen.Loaders.TableConfigLoader, Beef.CodeGen.Core">
  <Generate GenType="Beef.CodeGen.Generators.DatabaseGetCodeGenerator" Template="DbSpTableGet_sql.hbs" FileName="sp{{Parent.Name}}{{Name}}.sql" OutDir="{{Root.Company}}.{{Root.AppName}}.Database/Schema/{{Parent.Schema}}/Stored Procedures" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseGetCollCodeGenerator" Template="DbSpTableGetColl_sql.hbs" FileName="sp{{Parent.Name}}{{Name}}.sql" OutDir="{{Root.Company}}.{{Root.AppName}}.Database/Schema/{{Parent.Schema}}/Stored Procedures" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseCreateCodeGenerator" Template="DbSpTableCreate_sql.hbs" FileName="sp{{Parent.Name}}{{Name}}.sql" OutDir="{{Root.Company}}.{{Root.AppName}}.Database/Schema/{{Parent.Schema}}/Stored Procedures" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseUpdateCodeGenerator" Template="DbSpTableUpdate_sql.hbs" FileName="sp{{Parent.Name}}{{Name}}.sql" OutDir="{{Root.Company}}.{{Root.AppName}}.Database/Schema/{{Parent.Schema}}/Stored Procedures" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseUpsertCodeGenerator" Template="DbSpTableUpsert_sql.hbs" FileName="sp{{Parent.Name}}{{Name}}.sql" OutDir="{{Root.Company}}.{{Root.AppName}}.Database/Schema/{{Parent.Schema}}/Stored Procedures" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseDeleteCodeGenerator" Template="DbSpTableDelete_sql.hbs" FileName="sp{{Parent.Name}}{{Name}}.sql" OutDir="{{Root.Company}}.{{Root.AppName}}.Database/Schema/{{Parent.Schema}}/Stored Procedures" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseMergeCodeGenerator" Template="DbSpTableMerge_sql.hbs" FileName="sp{{Parent.Name}}{{Name}}.sql" OutDir="{{Root.Company}}.{{Root.AppName}}.Database/Schema/{{Parent.Schema}}/Stored Procedures" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseViewCodeGenerator" Template="DbVwTable_sql.hbs" FileName="{{ViewName}}.sql" OutDir="{{Root.Company}}.{{Root.AppName}}.Database/Schema/{{ViewSchema}}/Views" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseUdtCodeGenerator" Template="DbUdtTableList_sql.hbs" FileName="udt{{Name}}List.sql" OutDir="{{Root.Company}}.{{Root.AppName}}.Database/Schema/{{Schema}}/Types/User-Defined Table Types" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseTvpCodeGenerator" Template="DbTvpTable_cs.hbs" FileName="{{Tvp}}DataTvp.cs" OutDir="{{Root.Company}}.{{Root.AppName}}.Business/Data" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseEfModelGenerator" Template="DbEfModel_cs.hbs" FileName="{{EFModelName}}.cs" OutDir="{{Root.Company}}.{{Root.AppName}}.Business/Data/EfModel" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseEfModelBuilderCodeGenerator" Template="DbEfModelBuilder_cs.hbs" FileName="ModelBuilderExtensions.cs" OutDir="{{Root.Company}}.{{Root.AppName}}.Business/Data/EfModel" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseQueryViewCodeGenerator" Template="DbVwQuery_sql.hbs" FileName="{{ViewName}}.sql" OutDir="{{Root.Company}}.{{Root.AppName}}.Database/Schema/{{ViewSchema}}/Views" />
  <Generate GenType="Beef.CodeGen.Generators.DatabaseQueryCdcOutboxCodeGenerator" Template="DbSpQueryCdcOutbox_sql.hb" FileName="{{CdcName}}.sql" OutDir="{{Root.Company}}.{{Root.AppName}}.Database/Schema/{{CdcSchema}}/Stored Procedures" />
</Script>